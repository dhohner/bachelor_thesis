cookies={};cookies.addRecentlyVisited=function(nid,title,url){handleCookieValue("recentlyVisited","add",nid,title,url)};cookies.removeRecentlyVisited=function(nid){handleCookieValue("recentlyVisited","remove",nid)};cookies.getRecentlyVisitedValues=function(){return getCookieValues("recentlyVisited")};cookies.clearRecentlyVisited=function(){Cookies.remove('recentlyVisited',{path:''})}
function getCookieValues(cookieName){var cookie=Cookies.getJSON(cookieName);if(cookie===undefined){Cookies.set(cookieName,{values:[]});cookie=Cookies.getJSON(cookieName)};return cookie.values}
function handleCookieValue(cookieName,method,nid,title,url){var recentlyVisitedCookieValues=getCookieValues(cookieName),recentlyVisitedCookieList=[];if(method==="add")recentlyVisitedCookieList.push({nid:nid,title:title,url:url});for(var i=0;i<recentlyVisitedCookieValues.length;i++){var loopItem=recentlyVisitedCookieValues[i];if(loopItem.nid!==undefined&&loopItem.nid!==nid)recentlyVisitedCookieList.push({nid:loopItem.nid,title:loopItem.title,url:loopItem.url});if(recentlyVisitedCookieList.length===10)break};Cookies.set(cookieName,{values:recentlyVisitedCookieList})}
